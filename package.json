{
  "name": "react-webpack2-skeleton",
  "version": "1.0.0",
  "description": "Get started with React with Webpack2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "webpack-dev-server --config config/webpack.config.dev.js",
    "start": "nodemon server",
    "build": "rimraf build && webpack --config config/webpack.config.js",
    "build:server": "webpack --config config/webpack.config.server.js",
    "test": "nyc mocha src/**/*spec.js --compilers js:babel-register --require ./test/setup.js --require ./test/helpers.js --require ./test/dom.js --recursive",
    "tdd": "npm test -- --watch",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^2.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.2",
    "enzyme": "^2.8.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "ignore-loader": "^0.1.2",
    "jsdom": "^10.0.0",
    "mocha": "^3.3.0",
    "node-sass": "^4.5.0",
    "nyc": "^10.2.0",
    "postcss-loader": "^1.3.2",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "next",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "sinon": "^2.1.0",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "express": "^4.15.0",
    "file-loader": "^0.11.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-style-loader": "^2.0.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "next",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.2",
    "redux-promise-middleware": "^4.2.0",
    "redux-promises": "^1.0.0",
    "redux-thunk": "^2.2.0",
    "transit-immutable-js": "^0.7.0",
    "transit-js": "^0.8.846"
  }
}
